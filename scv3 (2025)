addcommand("kick",
"a",
function()

    if _G.debounce1 == true then 
        return 
    end
    _G.debounce1 = true

    task.delay(1, function()
        _G.debounce1 = false
    end)

    for _Index, Target in pairs(GetPlayer(args[2])) do

        local index = table.find(kickwl, Target)

        for i, v in ipairs(kickwl) do
            if v == Target.Name then
                return
            end
        end

        print(kickwl[index])

        chatt(_G.prefix.."unantipotato");wait()

        local kickfinished = false

        for i = 1, 3 do
            chatt("gear me "..antilog.."25741198")
        end

        local disccheck = true
        local spos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        local acheck = false
        _G.stable = true
        _G.stop = false
        task.wait()
        chatt("reset "..Target.Name)
        wait(.35)
        chatt("blind "..Target.Name)
        chatt("size "..Target.Name.." nan")
        --chatt("size me .3")
        task.wait()
        chatt("size "..Target.Name.." nan")
        wait(.35)
        chatt("freeze "..Target.Name)
        if not Target.Character:FindFirstChild("ice") or game.Players.LocalPlayer.Character:FindFirstChild("ice") then
            chatt("freeze "..Target.Name)
            task.wait()
        end
        if _G.antikick == true then
            _G.antikick = false
            acheck = true
            _G.stop = false
        else
            acheck = false
            _G.antikick = false
        end
        if _G.kcheck == true then
            coroutine.wrap(function()
                while _G.kcheck == true do task.wait(.3)
                    if _G.stable == false then task.wait(.4) end
                    if Target.Character:FindFirstChild("HotPotato") or Target.Backpack:FindFirstChild("HotPotato") then
                        break
                    end
                    if not Target.Character:FindFirstChild("ice") then
                        chatt("size "..Target.Name.." nan")
                        chatt("freeze "..Target.Name)
                        _G.stop = true
                        _G.stable = false
                        chatt(_G.prefix.."kick "..Target.Name.." fuck")
                    end
                    if Target.Character.Humanoid.Health == 0 then
                        chatt("respawn "..Target.Name)
                        _G.stop = true
                        _G.stable = false
                        _G.kcheck = false
                        task.wait()
                        chatt(_G.prefix.."kick "..Target.Name.." fuck")
                        _G.kcheck = true
                        _G.stable = true
                        _G.stop = false
                    end
                end
                end)()
        end
        wait(.2)
        if _G.stop == false and _G.stable == true then
            _G.stable = false
            chatt("ff "..Target.Name)
            for i = 1, 3 do
                chatt("gear me "..antilog.."25741198")
                kickfinished = true
            end
            chatt("gear me "..antilog.."25162389")
            chatt("gear me "..antilog.."25162389")

            if kickfinished then
                wait()
            end

            chatt(_G.prefix.."usetools");wait()

            wait(.1)

            chatt(_G.prefix.."usetools");wait()
            task.wait(.12)
            local v = Target.Character
            workspace.CurrentCamera.CameraSubject = v
            local Char = game.Players.LocalPlayer.Character or workspace:FindFirstChild(game.Players.LocalPlayer.Name)
            local hum = Char and Char:FindFirstChildWhichIsA('Humanoid')
            local hrp = hum and hum.RootPart
            local hrppos = hrp.CFrame
            hum = hum:Destroy() or hum:Clone()
            hum.Parent = Char
            for _, child in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if child:IsA("BasePart") and child.CanCollide == true then
                    child.CanCollide = false
                end
            end
            coroutine.wrap(function()
                game.Players.LocalPlayer.CharacterAdded:Wait():WaitForChild('Humanoid').RootPart.CFrame = hrppos
            end)()
            --chatt("invisible me")
            function getRoot(char)
                local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
                return rootPart
            end
            local vHRP = getRoot(v)
            while Char and Char.Parent and vHRP and vHRP.Parent do task.wait()
                local Tools = false
                for _, v in ipairs(Char:GetChildren()) do
                    if v:IsA('BackpackItem') and v:FindFirstChild('Handle') then
                        Tools = true
                        firetouchinterest(v.Handle, vHRP, 0)
                        firetouchinterest(v.Handle, vHRP, 1)
                    end
                end
                if not Tools then
                    break
                end
                --[[
                hrp.CFrame = vHRP.CFrame
                task.wait(.1)
                chatt("trip me")
                chatt("seizure me")
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = spos
                hrp.CFrame = vHRP.CFrame
                wait()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = spos
                hrp.CFrame = vHRP.CFrame
                wait()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = spos
                hrp.CFrame = vHRP.CFrame
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = spos
                ]] -- PLEASE UNCOMMENT ALL COMMENTED THINGS IN THE KICK COMMAND IF U HAVE NO FIRETOUCH OR WANT TO EXPERIMENT. IT WORKS PRETTY GOOD AND MAYBE EVEN BETTER THAT WAY
            end
            task.wait(.1)
            --chatt("reset me")
            if stopsound == true then
                chatt("clean")
            else
                chatt("ff Stopsound not enabled, therefore the sound will keep playing")
            end
            while disccheck == true do task.wait()
                if Target.Character:FindFirstChild("HotPotato") or Target.Backpack:FindFirstChild("HotPotato") then
                    if _G.kcheck == true then
                        _G.kcheck = false
                        _G.kcheck = true
                    end
                    if acheck == true and _G.antikick == false then
                        chatt(";antikick")
                        acheck = false
                    end
                    chatt("rainbowify "..Target.Name)
                    chatt("ff "..Target.Name)
                    chatt("name "..Target.Name.." Client status:\n inactive")
                    if stopsound == true then
                        chatt("clean up the potato mess                                                                       "..math.random(1,1000))
                    else
                        chatt("ff Stopsound not enabled, therefore the sound will keep playing")
                    end
                    disccheck = false
                    _G.stable = true
                elseif not Target.Backpack:FindFirstChild("HotPotato") or Target.Character:FindFirstChild("HotPotato") then
                    chatt(_G.prefix.."kick "..Target.Name.." fuck")
                    disccheck = false
                    if _G.kcheck == true and _G.stop == false and _G.stable == true then
                        chatt("reset "..Target.Name)
                    elseif _G.stable == false then
                        _G.kcheck = false
                    end
                    if acheck == true and _G.antikick == false and _G.kcheck == false then
                        acheck = false
                        chatt(_G.prefix.."antikick fuck")
                    end
                    print("Ran entirely")
                end
            end
        end
    end
end
)

