task.delay(2, function()
    local file = readfile("varstartups.txt")
    local commandqueue = {}
    local lastexecutedtime = tick()

    for command in file:gmatch("[^\r\n]+") do
        local waittime = string.match(command, "wait([%.%d]+)")
        if waittime then
            waittime = tonumber(waittime)
            lastexecutedtime = tick() + waittime
            table.insert(commandqueue, function()
                local startwaittime = tick()
                while tick() - startwaittime < waittime do
                    task.wait(waittime)
                end
            end)
        else
            table.insert(commandqueue, function()
                chatt(command)
                print(command)
            end)
        end
    end

    local function processcommandqueue()
        if #commandqueue > 0 then
            local nextcommand = table.remove(commandqueue, 1)
            nextcommand()
            processcommandqueue()
        end
    end

    processcommandqueue()
end)

